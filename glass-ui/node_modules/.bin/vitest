#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.4_@vitest+ui@1.6.1_happy-dom@18.0.1_jsdom@23.2.0_lightningcss@1.30.1/node_modules/vitest/node_modules:/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.4_@vitest+ui@1.6.1_happy-dom@18.0.1_jsdom@23.2.0_lightningcss@1.30.1/node_modules:/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.4_@vitest+ui@1.6.1_happy-dom@18.0.1_jsdom@23.2.0_lightningcss@1.30.1/node_modules/vitest/node_modules:/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.4_@vitest+ui@1.6.1_happy-dom@18.0.1_jsdom@23.2.0_lightningcss@1.30.1/node_modules:/Users/tuliopinheirocunha/tulio-brand-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
