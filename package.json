{
  "name": "@org/ui",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "svelte": "./dist/index.js",
  "sideEffects": false,
  "peerDependencies": {
    "svelte": ">=5.0.0",
    "@threlte/core": "^8.1.4",
    "@threlte/extras": "^9.4.4",
    "three": "^0.179.1",
    "lucide-svelte": "^0.375.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && bun run types:emit",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check": "bun run check:lib",
    "check:lib": "NODE_ENV=development svelte-check --no-tsconfig --workspace src/lib --diagnostic-sources svelte,css --ignore components/webgl --threshold error",
    "lint": "eslint 'src/lib/**/*.{ts,js,svelte}'",
    "lint:fix": "eslint 'src/lib/**/*.{ts,js,svelte}' --fix",
    "types:emit": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "docs:dev": "mintlify dev docs",
    "docs:links": "mintlify broken-links docs",
    "docs:preview": "mintlify preview docs",
    "histoire": "histoire dev",
    "histoire:build": "histoire build"
  },
  "devDependencies": {
    "@histoire/plugin-svelte": "^1.0.0-alpha.8",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "histoire": "^1.0.0-alpha.8",
    "lightningcss": "^1.30.1",
    "mintlify": "^4.2.61",
    "storybook": "^9.1.1",
    "svelte": "^5.38.0",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ]
}
