{
  "name": "@mintlify/cli",
  "version": "4.0.664",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mint": "bin/index.js",
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && NODE_NO_WARNINGS=1 node bin/index.js",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@mintlify/common": "1.0.481",
    "@mintlify/link-rot": "3.0.612",
    "@mintlify/models": "0.0.216",
    "@mintlify/prebuild": "1.0.601",
    "@mintlify/previewing": "4.0.648",
    "@mintlify/validation": "0.1.433",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "fs-extra": "^11.2.0",
    "ink": "^5.2.1",
    "inquirer": "^12.3.0",
    "js-yaml": "^4.1.0",
    "react": "^18.3.1",
    "semver": "^7.7.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config-typescript": "1.0.13",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/detect-port": "^1.3.2",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "openapi-types": "^12.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.4",
    "vitest-mock-process": "^1.0.4"
  },
  "gitHead": "42b3e30ae1313b5eb7655ac9b8de8bd51a7e387f"
}
